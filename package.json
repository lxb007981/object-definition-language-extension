{
  "name": "object-definition-language-extension",
  "displayName": "Object Definition Language",
  "description": "A Visual Studio Code extension with basic syntax highlighting for prplOS Object Definition Language.",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "LU, Xinbo",
  "repository": {
    "type": "git",
    "url": "https://github.com/lxb007981/object-definition-language-extension"
  },
  "icon": "resources/extension-icon.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "object-definition-language",
      "aliases": ["Object Definition Language", "object-definition-language"],
      "extensions": [".odl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "object-definition-language",
      "scopeName": "source.odl",
      "path": "./syntaxes/object-definition-language.tmLanguage.json"
    }]
  }
}
